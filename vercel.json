{
    "version": 2,
    "builds":[
        {
            "src" : "todolist/wsgi",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12"}
        }
        
        
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/todolist/wsgi.py"
        }
    ]
}